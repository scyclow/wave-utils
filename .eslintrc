{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "new-cap": 0,
    "prefer-const": 0,
    "react/prop-types": 0,
    "one-var": 0
  },
  "globals": {
    "React": false
  }
}
